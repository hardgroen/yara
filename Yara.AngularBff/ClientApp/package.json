{
  "name": "yara-spa-client",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "ng": "ng",
    "prestart": "node aspnetcore-https",
    "build": "ng build",
    "build:ssr": "ng run Yara.AngularClientBff:server:dev",
    "start": "run-script-os",
    "start:windows": "ng serve --port 44473 --ssl --ssl-cert \"%APPDATA%\\ASP.NET\\https\\%npm_package_name%.pem\" --ssl-key \"%APPDATA%\\ASP.NET\\https\\%npm_package_name%.key\"",
    "start:default": "ng serve --port 44473 --ssl --ssl-cert \"$HOME/.aspnet/https/${npm_package_name}.pem\" --ssl-key \"$HOME/.aspnet/https/${npm_package_name}.key\"",
    "deploy": "npm run write:env -s && ng deploy",
    "serve:sw": "npm run build -s && npx http-server ./dist -p 4200",
    "lint": "ng lint && stylelint \"src/**/*.scss\"",
    "test": "npm run write:env -s && ng test",
    "test:ci": "npm run write:env -s && npm run lint -s && ng run yara-spa-client:test:ci",
    "e2e": "npm run write:env -s && ng e2e",
    "cypress:open": "npm run write:env -s && ng run yara-spa-client:cypress-open",
    "cypress:run": "npm run write:env -s && ng run yara-spa-client:cypress-run",
    "translations:extract": "ngx-translate-extract --input ./src --output ./src/translations/template.json --format=json --clean --sort",
    "docs": "hads ./docs -o",
    "write:env": "ngx-scripts env npm_package_version",
    "prettier": "prettier --write \"./src/**/*.{ts,js,html,scss}\"",
    "prettier:check": "prettier --list-different \"./src/**/*.{ts,js,html,scss}\"",
    "postinstall": "npm run prettier -s && cd ..\\.. husky install Yara.AngularBff/ClientApp/.husky",
    "generate": "ng generate",
    "watch": "ng build --watch --configuration development"
  },
  "dependencies": {
    "@angular/animations": "~15.1.4",
    "@angular/cdk": "~15.1.4",
    "@angular/common": "~15.1.4",
    "@angular/compiler": "~15.1.4",
    "@angular/core": "~15.1.4",
    "@angular/forms": "~15.1.4",
    "@angular/localize": "~15.1.4",
    "@angular/material": "~15.1.4",
    "@angular/platform-browser": "~15.1.4",
    "@angular/platform-browser-dynamic": "~15.1.4",
    "@angular/platform-server": "^15.1.4",
    "@angular/router": "~15.1.4",
    "@angular/service-worker": "~15.1.4",
    "@azure/ng-deploy": "^0.2.3",
    "@ngx-translate/core": "^14.0.0",
    "lodash": "^4.17.20",
    "material-design-icons-iconfont": "^6.1.0",
    "moment": "^2.29.4",
    "rxjs": "^7.5.0",
    "tslib": "^2.3.0",
    "zone.js": "~0.11.4",
    "run-script-os": "^1.1.6"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~15.1.5",
    "@angular-eslint/builder": "~15.2.1",
    "@angular-eslint/eslint-plugin": "~15.2.1",
    "@angular-eslint/eslint-plugin-template": "~15.2.1",
    "@angular-eslint/schematics": "~15.2.1",
    "@angular-eslint/template-parser": "~15.2.1",
    "@angular/cli": "~15.1.5",
    "@angular/compiler-cli": "~15.1.4",
    "@angular/language-service": "~15.1.4",
    "@biesbjerg/ngx-translate-extract": "^7.0.3",
    "@biesbjerg/ngx-translate-extract-marker": "^1.0.0",
    "@cypress/schematic": "^2.0.3",
    "@ngneat/until-destroy": "^9.0.0",
    "@ngx-rocket/scripts": "^5.2.2",
    "@types/jasmine": "^4.0.0",
    "@types/jasminewd2": "^2.0.8",
    "@types/lodash": "^4.14.184",
    "@types/node": "^14.18.26",
    "@typescript-eslint/eslint-plugin": "~5.34.0",
    "@typescript-eslint/parser": "~5.34.0",
    "cypress": "~10.6.0",
    "eslint": "^8.3.0",
    "eslint-plugin-import": "latest",
    "eslint-plugin-jsdoc": "latest",
    "eslint-plugin-prefer-arrow": "latest",
    "hads": "^3.0.0",
    "https-proxy-agent": "^5.0.0",
    "husky": "^8.0.1",
    "jasmine-core": "~4.2.0",
    "jasmine-spec-reporter": "~7.0.0",
    "karma": "~6.4.0",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage": "~2.2.0",
    "karma-jasmine": "~5.1.0",
    "karma-jasmine-html-reporter": "~2.0.0",
    "karma-junit-reporter": "^2.0.1",
    "postcss": "^8.4.5",
    "prettier": "^2.2.1",
    "pretty-quick": "^3.1.0",
    "stylelint": "~14.11.0",
    "stylelint-config-prettier": "^9.0.3",
    "stylelint-config-recommended-scss": "~7.0.0",
    "stylelint-config-standard": "~28.0.0",
    "ts-node": "^10.1.0",
    "typescript": "~4.9.5"
  },
  "prettier": {
    "singleQuote": true,
    "overrides": [
      {
        "files": "*.scss",
        "options": {
          "singleQuote": false
        }
      }
    ]
  }
}
